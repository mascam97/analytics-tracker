{
    "name": "placetopay/analytics-tracker",
    "description": "Track user interactions or system events",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Placetopay\\AnalyticsTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Placetopay\\AnalyticsTracker\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "mixpanel/mixpanel-php": "^2.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.38"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Placetopay\\AnalyticsTracker\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php",
        "test": "vendor/bin/phpunit tests/"
    }
}
